{
  "cli": {
    "version": ">= 3.7.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "image": "ubuntu-20.04-jdk-11-ndk-r21e"
      },
      "ios": {
        "resourceClass": "m1-medium"
      },
      "channel": "development"
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "development-simulator"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      },
      "channel": "preview"
    },
    "production": {
      "ios": {
        "resourceClass": "m1-medium"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production":{
      "android": {
        "serviceAccountKeyPath": "path/to/serviceAccountKey.json",
      "track": "internal"
      },
      "ios": {
      "appleId": "yourappleid@apple.com",
      "ascAppId": "1234567890",
      "ascUsername": "yourappleid@apple.com",
      "appleTeamId": "YOUR_APPLE_TEAM_ID",
      "appleTeamName": "YOUR_APPLE_TEAM_NAME"
      }
    }
  }
}
